    "version": "0.2.0",
    "configurations": [
    {
        "name": "(gdb) QEMU",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceRoot}/bin/ozone-3.0.0.bin",
        "miDebuggerServerAddress": "localhost:3117",
        "args": [],
        "stopAtEntry": false,
        "cwd": "${workspaceRoot}",
        "targetArchitecture": "x64",
        "preLaunchTask": "dbg-serv",
        "setupCommands": [
            {
                "description": "Enable pretty-printing for gdb",
                "text": "-enable-pretty-printing",
                "ignoreFailures": false
            }
        ]
    }
    ]
}